enum class PaginationSize {
sm, lg, normal
}

<bs-pagination size="${PaginationSize = PaginationSize.normal}" class="${String = ''}" content="$Content">
    <nav aria-label="Page navigation">
        <ul class="pagination ${cssClass(If(size != PaginationSize.normal, 'pagination-$size'), `class`)}">
            $content
        </ul>
    </nav>
</bs-pagination>
